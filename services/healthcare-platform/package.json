{
  "name": "brainsait-healthcare-platform-unified",
  "version": "2.0.0",
  "description": "BrainSAIT Unified Healthcare Platform - AI-Powered Healthcare with NPHIES/FHIR Integration, Saudi Market Support, and Advanced Analytics",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "build:optimized": "NODE_ENV=production ANALYZE=false npm run build",
    "start": "next start",
    "export": "next export",
    "lint": "next lint",
    "lint:fix": "eslint . --ext .ts,.tsx,.js --fix",
    "type-check": "tsc --noEmit",
    "deploy:aws": "./scripts/deploy-aws.sh",
    "deploy:aws:freetier": "./scripts/deploy-aws-freetier.sh",
    "deploy:cloudflare": "./scripts/deploy-cloudflare.sh",
    "test": "jest",
    "test:e2e": "playwright test",
    "test:coverage": "jest --coverage",
    "analyze": "ANALYZE=true npm run build",
    "analyze:bundle": "npx @next/bundle-analyzer",
    "smoke": "node scripts/smoke.js",
    "security:send": "node scripts/security-send.js",
    "security:followups": "node scripts/security-followups.js",
    "security:channels": "node scripts/security-channels.js",
    "security:issues": "node scripts/security-issues.js",
    "security:thread:add": "node scripts/security-thread-add.js",
    "ci:lint": "eslint . --ext .ts,.tsx,.js",
    "ci:test": "jest --ci --coverage --watchAll=false",
    "ci:smoke": "node scripts/smoke.js",
    "ci": "npm run type-check && npm run ci:lint && npm run ci:test",
    "cost:monitor": "node scripts/cost-monitor.js",
    "validate:deployment": "node scripts/validate-deployment.js",
    "deployment:readiness": "node scripts/deployment-readiness.js",
    "health:check": "curl -f http://localhost:3000/api/health || exit 1",
    "optimize:images": "node scripts/optimize-images.js",
    "optimize:bundle": "node scripts/optimize-bundle.js"
  },
  "keywords": [
    "healthcare",
    "ai",
    "nphies",
    "fhir",
    "hipaa",
    "saudi-arabia",
    "medical-imaging",
    "transcription",
    "stripe-banking",
    "brainsait",
    "vision-2030",
    "arabic-support",
    "rtl",
    "medical-ai",
    "healthcare-analytics"
  ],
  "author": "BrainSAIT Healthcare Team",
  "license": "MIT",

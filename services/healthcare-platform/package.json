    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/lru-cache": "^7.10.9",
    "bcryptjs": "^2.4.3",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "fhir": "^4.11.1",
    "framer-motion": "^11.18.2",
    "jsonwebtoken": "^9.0.0",
    "lru-cache": "^11.2.1",
    "lucide-react": "^0.543.0",
    "next": "^15.4.6",
    "next-auth": "^4.24.0",
    "next-intl": "^4.3.4",
    "react": "^19.1.1",
